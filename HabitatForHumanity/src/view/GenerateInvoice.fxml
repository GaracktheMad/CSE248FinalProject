<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InvoiceGenerationController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Label alignment="CENTER" text="Invoice:" textAlignment="CENTER" underline="true" />
            <TextArea fx:id="invoiceField" editable="false" prefHeight="300.0" prefWidth="495.0" />
            <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnHome" text="Return" />
         </children>
         <StackPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </StackPane.margin>
      </VBox>
   </children>
</StackPane>
