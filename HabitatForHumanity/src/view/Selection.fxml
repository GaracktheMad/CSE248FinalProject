<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="selectionController">
	<children>
		<Label fx:id="name" text="Name" />
		<Label fx:id="id" text="ID" />
		<Button fx:id="select" mnemonicParsing="false" onAction="#detailSelect" text="Select" />
	</children>
</HBox>
